<%= form_for([:admin, @type],
             remote: true) do |f| %>
    <p id="message"></p>
    <div class='form-group label-floating'>
      <%= f.label :name, class: 'control-label' %>
      <%= f.text_field :name, class: 'form-control'%>
    </div>
    <%= f.submit class:'btn btn-primary' %>
<% end %>